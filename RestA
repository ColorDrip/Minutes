from office365.sharepoint.client_context import ClientContext
from office365.runtime.auth.user_credential import UserCredential

ctx = ClientContext("https://yourtenant.sharepoint.com/sites/IntranetSite").with_credentials(
    UserCredential("automation_user@domain.com", "your_password")
)
items = ctx.web.lists.get_by_title("Announcements").items.get().execute_query()
for item in items:
    print(item.properties["Title"])